{
  "expo": {
    "name": "bolt-expo-nativewind",
    "slug": "bolt-expo-nativewind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.odeiosbt.boltexponativewind",
      "config": {
        "googleSignIn": {
          "reservedClientId": "1070809826302-ios.apps.googleusercontent.com"
        }
      }
    },
    "android": {
      "package": "com.odeiosbt.boltexponativewind",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "17026e04-1fc9-4001-9cdd-2aa8a8f37b3f"
      },
      "expoClientId": "1070809826302-8b7842955a27d14fbc36af.apps.googleusercontent.com",
      "expoAndroidClientId": "1070809826302-android.apps.googleusercontent.com",
      "expoIosClientId": "1070809826302-ios.apps.googleusercontent.com"
    }
  }
}
